{
    "title": "House Passes Bill Raising Age For Semi-Automatic Gun Ownership To 21",
    "id": "STORY_116_DOCUMENT_0",
    "split": "dev",
    "date": "2022-06-08",
    "topic": "gun",
    "source": "The Daily Caller",
    "media_ideology": "Right",
